1.
const entries = [{ name: "Asha", late: false }, 

 { name: "Ravi", late: true }]; 

const entry = entries.some(e => e.late);

console.log(entry);

//Output:- true




2.
const entries = [{ name: "Asha", late: false }, 

 { name: "Ravi", late: true }]; 

const entry = entries.some(e => e.late == true);

console.log(entry);

//Output:- true
