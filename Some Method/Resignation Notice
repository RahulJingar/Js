1.
const resignations = [{ active: false }, 

 { active: true }];
 
const resignation = resignations.some(r => r.active);
console.log(resignation);

//Output:- true



2.
const resignations = [{ active: false }, 

 { active: true }];
 
const resignation = resignations.some(r => r.active == true);
console.log(resignation);

//Output:- true
