
1.
const students = [
  { id: 1, name: "Ishita", pendingAssignments: true },
  { id: 2, name: "Raghav", pendingAssignments: false },
  { id: 3, name: "Nina", pendingAssignments: true },
  { id: 4, name: "Sarthak", pendingAssignments: false }
];

const pendingAssignment = students.filter(s => s.pendingAssignments);

console.log(pendingAssignment);

/*Output:- [
  { id: 1, name: 'Ishita', pendingAssignments: true },
  { id: 3, name: 'Nina', pendingAssignments: true }
]
*/



2.
const students = [
  { id: 1, name: "Ishita", pendingAssignments: true },
  { id: 2, name: "Raghav", pendingAssignments: false },
  { id: 3, name: "Nina", pendingAssignments: true },
  { id: 4, name: "Sarthak", pendingAssignments: false }
];

const pendingAssignment = students.filter(s => s.pendingAssignments == true);

console.log(pendingAssignment);

/*Output:- [
  { id: 1, name: 'Ishita', pendingAssignments: true },
  { id: 3, name: 'Nina', pendingAssignments: true }
]
*/
