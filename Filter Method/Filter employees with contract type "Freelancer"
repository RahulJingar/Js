
const employees = [
  { id: 1, name: "Varun", contractType: "Permanent" },
  { id: 2, name: "Manasi", contractType: "Freelancer" },
  { id: 3, name: "Rishabh", contractType: "Freelancer" },
  { id: 4, name: "Neelam", contractType: "Intern" }
