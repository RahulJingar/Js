const staff = [
  { id: 1, name: "Pooja", role: "Nurse" },
  { id: 2, name: "Raj", role: "Doctor" },
  { id: 3, name: "Lata", role: "Nurse" },
  { id: 4, name: "Amit", role: "Receptionist" }
];


const nurse = staff.filter(s => s.role == 'Nurse');
console.log(nurse);

/*Output:- [
  { id: 1, name: 'Pooja', role: 'Nurse' },
  { id: 3, name: 'Lata', role: 'Nurse' }
        ]
*/
