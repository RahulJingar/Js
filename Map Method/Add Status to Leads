 const lead = ['Lead1', 'Lead2'];

 const status = lead.map(l => `{name: '${l}', status: new}`);

 console.log(status);

 //Output:- [ "{name: 'Lead1', status: new}", "{name: 'Lead2', status: new}" ]

2.
