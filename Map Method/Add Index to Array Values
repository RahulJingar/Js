
 1.
 const arr = ['a', 'b'];

 const indexOfArray = arr.map((index,value) => {
     return `${value}, ${index}`
 });

 console.log(indexOfArray);

 //Output:- [ '0, a', '1, b' ]


2.
const arr = ['a', 'b'];

const indexOfArray = arr.map((index,value) => `${value}, ${index}`);

console.log(indexOfArray);

//Output:- [ '0, a', '1, b' ]











