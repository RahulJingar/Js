1.
 const values = [10, 45, 23, 89];

 const min = values.reduce((acc,val) => acc > val ? val : acc);

 console.log("Minimum Value is:", min);

//Output:- Minimum Value is: 10

2.
const values = [10, 45, 23, 89];

const minimum = values.reduce((acc,val) => {
    const min = acc > val ? val : acc;
    return min;
});

console.log("Minimum Value is:", minimum);

//Output:- Minimum Value is: 10
