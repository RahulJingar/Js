
let src = {
    name:"Rahul",
    age:"17",
    city:"jaipur",
}

let dest = {};

for(let key in src){
    let newKey = key;
    let newValue = src[key];

    dest[newKey] = newValue;
}

src.age = 90

console.log("src:",src);  
console.log("dest:",dest); 

//Output:- src: {name: 'Rahul', age: 90, city: 'jaipur'}
// dest: {name: 'Rahul', age: '17', city: 'jaipur'} 
